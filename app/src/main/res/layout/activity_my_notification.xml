<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.patryko.testing_notification2.MyNotification">


    <TextView
        android:id="@+id/activity_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="137dp"
        android:layout_marginStart="137dp"
        android:layout_marginTop="178dp" />

    <TextView
        android:id="@+id/activity_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_above="@+id/activity_title"
        android:layout_alignLeft="@+id/activity_title"
        android:layout_alignStart="@+id/activity_title"
        android:layout_marginBottom="37dp" />

    <TextView
        android:id="@+id/activity_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/activity_title"
        android:layout_alignRight="@+id/activity_title"
        android:layout_centerVertical="true"
        android:text="TextView" />





</RelativeLayout>
